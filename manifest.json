{
  "manifest_version": 1,
  "name": "JB Power up 1",
  "description": "A custom Power-Up for Trello",
  "author": "JB",
  "capabilities": ["board-buttons", "card-buttons", "show-settings"],
  "icon": {
    "url": "https://magicmoam.github.io/trello_txt_card/icon.png"
  },
  "connectors": {
    "iframe": {
      "url": "https://magicmoam.github.io/trello_txt_card/connector.html"
    }
  },
  "board-buttons": [
    {
      "icon": "https://magicmoam.github.io/trello_txt_card/icon.png",
      "text": "Create Cards",
      "callback": {
        "url": "https://magicmoam.github.io/trello_txt_card/popup.html",
        "height": 184
      }
    }
  ]
}

